<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MauiApp1.ListPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    Title="ListPage">
    <ListView ItemsSource="{Binding lives}" ItemTapped="ListView_ItemTapped">
        <ListView.ItemTemplate>
            <DataTemplate>
                <TextCell Detail="{Binding Watching}" Text="{Binding Path}" />
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>